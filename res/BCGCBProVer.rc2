// Macros

#ifndef BCGP_VERSION_STRING
    #define BCGP_VERSION_STRING_TEMP(MAJOR_H, MAJOR_L, MINOR_H, MINOR_L) #MAJOR_H ", " #MAJOR_L ", " #MINOR_H ", " #MINOR_L
    #define BCGP_VERSION_STRING(MAJOR_H, MAJOR_L, MINOR_H, MINOR_L) BCGP_VERSION_STRING_TEMP(MAJOR_H, MAJOR_L, MINOR_H, MINOR_L)
#endif

#if defined(BCGP_VERSION_FILE_MAJOR_H) && defined(BCGP_VERSION_FILE_MAJOR_L) && defined(BCGP_VERSION_FILE_MINOR_H) && defined(BCGP_VERSION_FILE_MINOR_L)
    #ifndef BCGP_VERSION_FIXED_FILEVERSION
        #define BCGP_VERSION_FIXED_FILEVERSION BCGP_VERSION_FILE_MAJOR_H,BCGP_VERSION_FILE_MAJOR_L,BCGP_VERSION_FILE_MINOR_H,BCGP_VERSION_FILE_MINOR_L
        #define BCGP_VERSION_FILEVERSION BCGP_VERSION_STRING(BCGP_VERSION_FILE_MAJOR_H,BCGP_VERSION_FILE_MAJOR_L,BCGP_VERSION_FILE_MINOR_H,BCGP_VERSION_FILE_MINOR_L)
    #endif
