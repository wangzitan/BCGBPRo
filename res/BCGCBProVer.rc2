#ifndef BCGP_VERSION_FIXED_FILEVERSION
    #error BCGP_VERSION_FIXED_FILEVERSION not defined.
#endif

#if defined(BCGP_VERSION_PRODUCT_MAJOR_H) && defined(BCGP_VERSION_PRODUCT_MAJOR_L) && defined(BCGP_VERSION_PRODUCT_MINOR_H) && defined(BCGP_VERSION_PRODUCT_MINOR_L)
    #ifndef BCGP_VERSION_FIXED_PRODUCTVERSION
        #define BCGP_VERSION_FIXED_PRODUCTVERSION BCGP_VERSION_PRODUCT_MAJOR_H,BCGP_VERSION_PRODUCT_MAJOR_L,BCGP_VERSION_PRODUCT_MINOR_H,BCGP_VERSION_PRODUCT_MINOR_L
        #define BCGP_VERSION_PRODUCTVERSION BCGP_VERSION_STRING(BCGP_VERSION_PRODUCT_MAJOR_H,BCGP_VERSION_PRODUCT_MAJOR_L,BCGP_VERSION_PRODUCT_MINOR_H,BCGP_VERSION_PRODUCT_MINOR_L)
    #endif
#endif

#ifndef BCGP_VERSION_FIXED_PRODUCTVERSION
    #error BCGP_VERSION_FIXED_PRODUCTVERSION not defined.
#endif

#ifndef BCGP_VERSION_CHARSET
    #ifdef _UNICODE
        #define BCGP_VERSION_CHARSET 0x04B0 // Unicode
		#define BCGP_VERSION_LANGID 0x0409 // U.S. English
		#define BCGP_VERSION_LANGUAGE_BLOCK "040904B0"
    #else
        #define BCGP_VERSION_CHARSET 0x04E4 // Multilingual
		#define BCGP_VERSION_LANGID 0x0409 // U.S. English
		#define BCGP_VERSION_LANGUAGE_BLOCK "040904E4"
    #endif
#endif

#if !defined(AFX_RESOURCE_DLL)

#if defined(AFX_TARG_ENU)
LANGUAGE LANG_ENGLISH, SUBLANG_ENGLISH_US
#pragma code_page(1252)
#endif

VS_VERSION_INFO VERSIONINFO
    FILEVERSION    BCGP_VERSION_FIXED_FILEVERSION
    PRODUCTVERSION BCGP_VERSION_FIXED_PRODUCTVERSION
    FILEFLAGSMASK  VS_FFI_FILEFLAGSMASK
#ifdef _DEBUG
	FILEFLAGS      VS_FF_DEBUG
#else
    FILEFLAGS      0L
#endif
    FILEOS         VOS_NT_WINDOWS32
#ifdef BCGP_VERSION_FILETYPE_APP
    FILETYPE       VFT_APP
#else
    FILETYPE       VFT_DLL
#endif
    FILESUBTYPE    0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
    BLOCK BCGP_VERSION_LANGUAGE_BLOCK
        BEGIN
#ifdef BCGP_VERSION_COMPANYNAME
            VALUE "CompanyName"     , BCGP_VERSION_COMPANYNAME
#endif
#ifdef BCGP_VERSION_FILEDESCRIPTION
            VALUE "FileDescription" , BCGP_VERSION_FILEDESCRIPTION
#endif
            VALUE "FileVersion"     , BCGP_VERSION_FILEVERSION
#ifdef BCGP_VERSION_INTERNALNAME
            VALUE "InternalName"    , BCGP_VERSION_INTERNALNAME
#endif
#ifdef BCGP_VERSION_LEGALCOPYRIGHT
            VALUE "LegalCopyright"  , BCGP_VERSION_LEGALCOPYRIGHT
#endif
#ifdef BCGP_VERSION_ORIGINALFILENAME
            VALUE "OriginalFilename", BCGP_VERSION_ORIGINALFILENAME
#endif
#ifdef BCGP_VERSION_PRODUCTNAME
            VALUE "ProductName"     , BCGP_VERSION_PRODUCTNAME
#endif
            VALUE "ProductVersion"  , BCGP_VERSION_PRODUCTVERSION
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", BCGP_VERSION_LANGID, BCGP_VERSION_CHARSET
    END
END

#endif
